var VAR_TYPE_STREETS = "streets"
var VAR_TYPE_BLOCK   = "block"
var VAR_TYPE_PARCELS = "parcels"
var VAR_TYPE_LOTS    = "lots"

class VarStreets
{
	init(streets)
	{
		this.streets = streets

		this.is_major = true
	}
}

class VarBlock
{
	init(block)
	{
		this.block = block

		this.offset = 0
	}
}

class VarParcels
{
	init(parcels)
	{
		this.parcels = parcels
	}
}