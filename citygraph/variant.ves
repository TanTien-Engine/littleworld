var VAR_TYPE_STREETS = "streets"
var VAR_TYPE_BLOCK   = "block"

var ROAD_TYPE_MAJOR = "major"
var ROAD_TYPE_MINOR = "minor"
var ROAD_TYPE_TINY  = "tiny"
var ROAD_TYPE_RIVER = "river"

var BLOCK_TYPE_COMMON = "common"
var BLOCK_TYPE_PARK   = "park"

class VarStreets
{
	init(streets, type)
	{
		this.streets = streets
		this.type = type
	}
}

class VarBlock
{
	init(poly)
	{
		this.poly = poly
		this.type = BLOCK_TYPE_COMMON
	}

	init(poly, type)
	{
		this.poly = poly
		this.type = type
	}
}